{
  "name": "react31032020",
  "version": "1.0.0",
  "description": "Учебный проект",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --mode development --open",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production",
    "test": "jest",
    "prettier": "prettier --check ./src/**/*.ts",
    "prettier-fix": "prettier --write .",
    "eslint": "npx eslint ./src/**/*.ts",
    "eslint-fix": "npx eslint --fix ./src/**/*.ts",
    "calc": "npx ts-node src/calc/index.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki-update": "npx loki update",
    "loki-test": "npx loki test",
    "loki-approve": "npx loki approve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxim1989/react31032020.git"
  },
  "keywords": [
    "otus",
    "reactjs",
    "redux",
    "webpack",
    "babel",
    "storybook"
  ],
  "author": "Fedorov Maksim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxim1989/react31032020/issues"
  },
  "homepage": "https://github.com/maxim1989/react31032020#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@redux-saga/testing-utils": "^1.1.3",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack": "^4.41.10",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.2.7",
    "jest-emotion": "^10.0.32",
    "loki": "^0.21.1",
    "prettier": "2.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.ts"
    ],
    "transform": {
      "^.+\\.ts(x?)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/src/shared$1"
    },
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/ramda": "^0.27.6",
    "@types/react-redux": "^7.1.9",
    "lodash": "^4.17.19",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "readline": "^1.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
